	"DynamicTokens":
	[
	// World
		{
			"Name": "SeasonalRainToken",
			"Value": "{{season}}"
  		},
  		{
			"Name": "SeasonalRainToken",
			"Value": "rain",
			"When": {
			  	"Weather |contains=Rain, Storm": true,
			  	"Season": "Spring, Summer, Fall"
			}
  		},
		{
			"Name": "IsOddYear",
			"Value": "{{Query: {{Year}} % 2 = 1}}"
		},

	// RSV-specific
		{
			"Name": "JioMasked",
			"Value": "false"
		},
		{
			"Name": "JioMasked",
			"Value": "true",
			"When":
			{
				"HasSeenEvent|contains=75160263": false
			}
		},
		{
			"Name": "JioMasked",
			"Value": "true",
			"When":
			{
				"LocationName |contains=Custom_Ridgeside_Ridge, Custom_Ridgeside_RidgeForest, Custom_Ridgeside_RSVSpiritRealm, Custom_Ridgeside_RSVNinjaHouse, Custom_Ridgeside_RidgeFalls Temp": true
			}
		},
		{
			"Name": "JioMasked",
			"Value": "true",
			"When":
			{
				"Hearts:Jio": "{{Range: 0,7}}"
			}
		},
		{
			"Name": "JioMasked",
			"Value": "true",
			"When":
			{
				"Hearts:Jio": "{{Range: 8,10}}",
				"Query: {{Random:{{Range:1,10}}|key=JioMask}}|contains= 4,5,6": false,
			}
		},
		{
			"Name": "JioMasked",
			"Value": "false",
			"When":
			{
				"Relationship:Jio": "Married",
				"LocationName |contains= Farm, FarmHouse, Summit": true
			}
		},

