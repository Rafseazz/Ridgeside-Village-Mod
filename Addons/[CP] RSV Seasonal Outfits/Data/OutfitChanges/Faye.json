			"When": {
				"Faye": "enabled",
                "Weather|contains = Rain, Storm": false,
				"IsOutdoors": true,
				"HasValue:{{DayEvent}}": false,
				"Query: {{Random:{{Range:1,10}}|key=FayeHat}} > 5": true,
			},
			"Update": "OnLocationChange",
			"PatchMode": "Overlay"
		},
		
